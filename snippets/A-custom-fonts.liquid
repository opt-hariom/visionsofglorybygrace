{% comment %}
	/* Proxima Nova: https://db.onlinewebfonts.com/c/bf9f5d50c1b928ff21436517a1a95ad9?family=Proxima+Nova */
	@font-face {
		font-family: 'Proxima Nova';
		font-weight: 400;
		font-style: normal;
		font-display: swap;
		src: url({{'Proxima_Nova.eot'|file_url}});
		src: url({{'Proxima_Nova.eot'|file_url|append:'?#iefix'}}) format('embedded-opentype'),
			url({{'Proxima_Nova.woff'|file_url}}) format('woff'), url({{'Proxima_Nova.woff2'|file_url}}) format('woff2'),
			url({{'Proxima_Nova.ttf'|file_url}}) format('truetype');
	}

	/* Proxima Nova Thin: https://db.onlinewebfonts.com/c/1e7e97d0da36ea1b00a66348a43dfb26?family=Proxima+Nova+Thin */
